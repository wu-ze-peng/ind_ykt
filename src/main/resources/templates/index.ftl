						map.centerAndZoom(point, 16);
						//map.enableScrollWheelZoom();  //滚轮控制缩小放大
						var marker = new BMap.Marker(point); // 创建标注
						map.addOverlay(marker); // 将标注添加到地图中


						var opts = {
							width: 200, // 信息窗口宽度
							height: 100, // 信息窗口高度
							title: "优科泰科技有限公司", // 信息窗口标题
						}
						var infoWindow = new BMap.InfoWindow("公司地址：广东省深圳市xxxxxx", opts); // 创建信息窗口对象
						marker.addEventListener("mouseover", function() {
							map.openInfoWindow(infoWindow, point); //开启信息窗口
						});

						marker.setAnimation(BMAP_ANIMATION_BOUNCE); //跳动的动画
						map.addControl(new BMap.MapTypeControl()); //添加地图类型控件
						map.setCurrentCity("深圳"); // 设置地图显示的城市 此项是必须设置的
						//创建和初始化地图函数：
						function initMap() {
							addMapControl(); //向地图添加控件
						}

						//地图控件添加函数：
						function addMapControl() {
							//向地图中添加缩放控件
							var ctrl_nav = new BMap.NavigationControl({
								anchor: BMAP_ANCHOR_TOP_LEFT,
								type: BMAP_NAVIGATION_CONTROL_LARGE
							});
							map.addControl(ctrl_nav);
							//向地图中添加缩略图控件
							var ctrl_ove = new BMap.OverviewMapControl({
								anchor: BMAP_ANCHOR_BOTTOM_RIGHT,
								isOpen: 1
							});
							map.addControl(ctrl_ove);
							//向地图中添加比例尺控件
							var ctrl_sca = new BMap.ScaleControl({
								anchor: BMAP_ANCHOR_BOTTOM_LEFT
							});
							map.addControl(ctrl_sca);
						}
						marker.addEventListener("click", function(e) {
							searchInfoWindow.open(marker);
						});
						map.addOverlay(marker); //在地图中添加marker
						initMap(); //创建和初始化地图
					}

					mapshow();


					//
					var myIndexScroll = function(element, className, parentEle) {

						var mainEvent = function() {
							//页面卷曲高度
							var scrollHeight = $(window).scrollTop();

							//顶部距文章顶部高度
							var elementTopToTop = element.offset().top
							//显示器高度
							var screenH = $(window).height()
							//当元素从上往下出现在屏幕中
							if (elementTopToTop - scrollHeight >= 0 && elementTopToTop - scrollHeight <= screenH) {
								element.addClass('animated ' + className).css('opacity', '1');
							}
						}

						mainEvent();

						$(window).scroll(function() {

							mainEvent();

						})



					}




					// 输入 做动画的元素，出现动画，离开动画 ，动画元素的父容器（有关纵方向的动画都需要加父容器）
					//
					myIndexScroll($('.products  .container'), 'fadeInLeft slow');
					myIndexScroll($('.news .container'), 'fadeInRight slow');


					myIndexScroll($('.company'), 'fadeInUp slow');

					myIndexScroll($('.index_honor_main'), 'zoomIn slow');

					myIndexScroll($('.contact_left'), 'fadeInLeft slow');

					myIndexScroll($('.contact_right'), 'fadeInRight slow');

					myIndexScroll($('.banner .index_zi h3'), 'fadeInDown slow');
					myIndexScroll($('.banner .index_zi p'), 'fadeInDown slow');
					myIndexScroll($('.banner .index_zi .banner_more'), 'fadeIn slow');





					var indexScroll = function() {

						$(window).on('scroll', function() {
							var scrollHeight = $(window).scrollTop();

							// 元素距离顶部高度
							var list_nameTop = $(".list_name").offset().top;


							if (list_nameTop - scrollHeight > 0 && list_nameTop - scrollHeight < $(window).height()) {
								$('.list_name').css("bottom", "15%")
							} else {
								$('.list_name').css("bottom", "0")
							}

						})
					}
					indexScroll();
					
				})
			</script>

